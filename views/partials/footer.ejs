                    <div class="footer_main_title py-3"> Category</div>
                    <ul class="footer_menu">
                        <% for(loopedAPI in apiEndpoints) {%>
                        <li><a href="/category/<%= loopedAPI %>" class=""><i class="fa fa-angle-right"></i>&nbsp;&nbsp; <%= apiEndpoints[loopedAPI] %></a></li>
                        <% } %>
                    </ul>
                </div>
    
                <!-- Nasional Posts Section -->
                <div class="col-12 col-md-5 col-lg-3 position_footer_relative">
                    <div class="footer_main_title py-3 "> Nasional</div>
                    <% for (let i = 0; i < 3; i++) { %>
                        <% let newDate = nasionalContent[index3].isoDate; %>
                        <% let dateObject = new Date(newDate); %>
                        <% let options = { year: 'numeric', month: 'long', day: 'numeric' }; %>
                        <% let formattedDate = dateObject.toLocaleDateString('en-US', options); %>

                        <div class="footer_makes_sub_font"><%= formattedDate %></div>
                        <a href="<%= nasionalContent[index3].link %>" class="footer_post pb-4"> <%= nasionalContent[index3].title %> </a>
                        <% index3++ %>
                    <% } %>
                    <div class="footer_position_absolute"><img src="/images/footer_sub_tipik.png" alt="img" class="width_footer_sub_img"/></div>
                </div>
            </div>
        </div>
